                                          <thead>
                                            <tr>
                                              <th>Nome</th>
                                              <th>Hotel</th>
                                              <th>Setor</th>
                                              <th>Transporte</th>
                                              <th>Premio</th>
                                              <th>Check</th>
                                              <th colspan="3"></th>
                                            </tr>
                                          </thead>

                                          <tbody>
                                            <% @colaboradors.each do |colaborador| %>
                                              <tr>
                                                <td><%= colaborador.nome %></td>
                                                <td><%= colaborador.hotel %></td>
                                                <td><%= colaborador.setor %></td>
                                                <td><%= colaborador.transporte %></td>
                                                <td><%= colaborador.preimo %></td>
                                                <td><%= colaborador.checkin %></td>
                                                <td><%= link_to 'Exibir', colaborador %></td>
                                                <td><%= link_to 'Editar', edit_colaborador_path(colaborador) %></td>
                                                <td><%= link_to 'Excluir', colaborador, method: :delete, data: { confirm: 'confirmar' } %></td>
                                              </tr>
                                            <% end %>
                                          </tbody>
                                        </table>
                                   </div>
                                        <br>

                                        <%= link_to 'Novo Colaborador', new_colaborador_path %>
 
         
